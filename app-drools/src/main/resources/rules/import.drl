package com.rules.impor
dialect "java"
import com.mycompany.app.bean.People
import function com.mycompany.app.utils.DroolsStringUtils.isEmpty


function String hello(String applicantName) {
    return "Hello " + applicantName + "!";
}

rule "impot"

  when
    $p : People(drlType == "impot")
  then
    System.out.println(isEmpty("我"));
    System.out.println(hello("达"));
  end
